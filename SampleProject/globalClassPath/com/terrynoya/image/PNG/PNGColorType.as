package com.terrynoya.image.PNG
{
	/**
	 * 0	GrayScale
	 * 2	RGB Triple
	 * 3	Palette
	 * 4	GrayScale with Alpha Channel
	 * 6	RGB	width Alpha Channel
	 */			
	public class PNGColorType
	{
		public static const GRAY_SCALE:int = 0;
		public static const RGB_TRIPLE:int = 2;
		public static const PALETTE:int = 3;
		public static const GRAY_SCALE_ALPHA:int = 4;
		public static const RGB_ALPHA:int = 6;
	}
}